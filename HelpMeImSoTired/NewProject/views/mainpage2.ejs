<%- include('partials/HomepageHeader') %> 
<%- include('partials/NavBar') %>

<div class="container">

  <!-- Image Gallery (15 images) -->
  <div>
    <% for(let i = 15; i < 30; i++) { %>
      <% if (i % 3 === 0) { %>
        <div class="row">
      <% } %>
      <div class="col-lg-4 col-md-6 col-sm-6 col-xs-12 mb-4">
        <div class="content-wrapper">
          <div class="subimage-box">
            <a href="/product/<%= productIds[i]%>">
              <img src="<%= imageUrls[i] %>" alt="Image <%= i + 1 %>" class="img-fluid"/>
            </a>
          </div>
        </div>
      </div>
      <% if ((i + 1) % 3 === 0 || i === 14) { %>
        </div>
      <% } %>
    <% } %>
  </div>
</div>

<%- include('partials/footer') %>